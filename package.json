{
  "name": "daaas-frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000/",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "connected-react-router": "^5.0.1",
    "custom-event-polyfill": "^1.0.7",
    "express": "^4.16.4",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.6.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.0",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "react-redux": "^5.1.1",
    "react-redux-toastr": "^7.4.10",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "single-spa": "^4.3.3",
    "typeface-roboto": "0.0.54",
    "typescript": "3.4.5"
  },
  "scripts": {
    "lint:js": "eslint --max-warnings=0 --ext=tsx --ext=js --ext=jsx --fix ./src",
    "lint:cypress": "eslint --ext=tsx --ext=js --ext=jsx --fix ./cypress",
    "serve:plugins": "node micro-frontend-tools/serve-plugins.js",
    "start": "concurrently \"npm run serve:plugins\" \"node server/auth-server.js\" \"react-scripts start\"",
    "build": "react-scripts build",
    "build:e2e": "cross-env GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "e2e:serve": "npm run build:e2e && node ./server/e2e-test-server.js",
    "e2e:interactive": "start-server-and-test e2e:serve http://localhost:3000 cy:open",
    "e2e": "start-server-and-test e2e:serve http://localhost:3000 cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "lint-staged": {
    "src/**/*.{tsx,js,jsx,json}": [
      "eslint --max-warnings=0 --ext=tsx --ext=js --ext=jsx --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "cypress/**/*.{tsx,js,jsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{tsx,js,jsx}",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/setupTests.js",
      "!src/state/reducers/App.reducer.tsx",
      "!src/stories/index.js",
      "!src/stories/utils.tsx",
      "!src/stories/storybookResourceStrings.tsx",
      "!**/*.stories.*",
      "!src/state/actions/loadMicroFrontends.tsx"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.1",
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.12",
    "@types/js-cookie": "^2.2.2",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/loglevel": "^1.5.4",
    "@types/node": "12.0.0",
    "@types/react": "16.8.16",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "^7.0.8",
    "@types/react-redux-toastr": "^7.4.1",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.3",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-mock-store": "^1.0.1",
    "@types/single-spa-react": "^2.8.3",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "redux-mock-store": "^1.5.3",
    "serve": "^11.0.0",
    "start-server-and-test": "^1.9.0",
    "storybook-react-router": "^1.0.5"
  }
}
